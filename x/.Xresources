XTerm*metaSendsEscape: true
XTerm*locale: true
xterm*termName: xterm-256color
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

xterm*VT100.geometry:     80x25
xterm*faceName:           Inconsolata for Powerline:style=Medium:size=12:antialias=true
xterm*dynamicColors:      true
!xterm*utf8:               2
xterm*eightBitInput:      true
xterm*saveLines:          4096
xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          false
xterm*rightScrollBar:     false
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*toolBar:            false


XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

URxvt*font: xft:Literation Mono Powerline:size=10:antialias=true:hinting=true
URxvt.scrollBar: off
URxvt*loginShell:true
URxvt*termName:rxvt-unicode
URxvt.urlLauncher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1
URxvt.underlineURLs: true


!-bg #333333 -fg #dedede -hlbg #d64937 -hlfg #dedede 
    rofi.opacity: 90
    rofi.width: 50
    rofi.lines: 5
    rofi.columns: 1
    rofi.fg: #dedede
    rofi.bg: #333333
    rofi.hlfg: #dedede
    rofi.hlbg: #d64937
    rofi.bw: 2
    rofi.loc: 0
    rofi.padding: 2
    rofi.yoffset: -2
    rofi.xoffset: 0
    rofi.fixed-num-lines: false
    rofi.hmode: false
    rofi.terminal: xterm
    rofi.ssh-set-title: true


















































































